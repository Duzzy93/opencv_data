   "source": [
    "import tkinter as tk\n",
    "\n",
    "# 창 생성\n",
    "window = tk.Tk()\n",
    "window.title(\"나의 첫 번째 윈도우 창\")\n",
    "window.geometry(\"400x300\")  # 가로 400, 세로 300\n",
    "\n",
    "# 라벨 추가\n",
    "label = tk.Label(window, text=\"Hello, Python GUI!\", font=(\"Arial\", 16))\n",
    "label.pack(pady=20)\n",
    "\n",
    "# 윈도우 실행\n",
    "window.mainloop()"
   ]
  }
 ],
